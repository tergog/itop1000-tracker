{
  "name": "itop1000_tracker",
  "version": "0.0.4",
  "main": "app.js",
  "description": "Build and package app for win32",
  "author": {
    "name": "ITOP1000",
    "email": "stepanenkoo.vsevolod@gmail.com"
  },
  "iohook": {
    "targets": [
      "node-82",
      "electron-82"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "linux-electron-packager": "electron-packager . itop1000_tracker --platform linux --asar --overwrite --arch x64 --out .",
    "electron-installer-debian": "electron-installer-debian --src ./itop1000_tracker-linux-x64/ --dest ./linux-installers/ --arch amd64",
    "win-electron-packager": "electron-packager . itop1000_tracker --platform win32 --arch x64 --out .",
    "electron-installer-windows": "node electron/electron-winstaller.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@google-cloud/storage": "^5.7.0",
    "asar": "^3.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "iohook": "^0.9.0",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.1",
    "mongoose": "^5.11.9",
    "ngx-electron": "^2.2.0",
    "npm-lifecycle": "^3.1.5",
    "prebuild-install": "^6.0.0",
    "robotjs": "^0.6.0",
    "rxjs": "~6.6.0",
    "serialport": "^9.0.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "electron": "^10.1.0",
    "electron-builder": "^22.9.1",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^2.3.4",
    "electron-winstaller": "^5.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-abi": "^2.19.3",
    "node-gyp": "^7.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
